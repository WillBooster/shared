{
  "name": "@willbooster/shared-lib-react",
  "version": "0.0.0-semantically-released",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.mts",
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs"
    }
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/cjs/index.d.cts",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "build-ts lib",
    "build-storybook": "build-storybook",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "lint": "eslint --color \"./{scripts,src,tests}/**/*.{cjs,cts,js,jsx,mjs,mts,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prettify": "prettier --cache --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,js,json,json5,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "storybook": "start-storybook -p 6006",
    "test": "vitest",
    "test/ci": "yarn build-storybook && vitest",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack4": "^6.5.16",
    "@storybook/manager-webpack4": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/eslint": "8.4.10",
    "@types/micromatch": "4.0.2",
    "@types/prettier": "2.7.2",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "@willbooster/eslint-config-ts": "10.0.8",
    "@willbooster/prettier-config": "9.1.1",
    "babel-loader": "^8.3.0",
    "build-ts": "5.1.0",
    "eslint": "8.33.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-sort-class-members": "1.16.0",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "eslint-plugin-storybook": "0.6.10",
    "eslint-plugin-unicorn": "45.0.2",
    "lint-staged": "13.1.0",
    "micromatch": "4.0.5",
    "prettier": "2.8.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sort-package-json": "2.3.0",
    "typescript": "4.9.4",
    "vitest": "0.28.3"
  },
  "peerDependencies": {
    "react": "~18.2.0",
    "react-dom": "~18.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
