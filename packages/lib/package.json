{
  "name": "@willbooster/shared-lib",
  "version": "0.0.0-semantically-released",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "rm -Rf dist; rollup -c --environment NODE_ENV:production",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "lint": "eslint --color \"./{src,__tests__,scripts}/**/*.{cjs,js,jsx,mjs,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prettify": "prettier --color --write \"**/{.*/,}*.{cjs,css,htm,html,js,json,json5,jsx,md,mjs,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-numeric-separator": "7.18.6",
    "@babel/plugin-transform-runtime": "7.19.6",
    "@babel/preset-env": "7.19.4",
    "@babel/preset-typescript": "7.18.6",
    "@rollup/plugin-commonjs": "22.0.2",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "14.1.0",
    "@types/babel__core": "7.1.19",
    "@types/babel__preset-env": "7.9.2",
    "@types/eslint": "8.4.8",
    "@types/micromatch": "4.0.2",
    "@types/prettier": "2.7.1",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "@willbooster/babel-configs": "1.3.0",
    "@willbooster/eslint-config-ts": "10.0.7",
    "@willbooster/prettier-config": "9.1.1",
    "@willbooster/rollup-configs": "3.0.1",
    "babel-plugin-transform-remove-console": "6.9.4",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-sort-class-members": "1.15.2",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "eslint-plugin-unicorn": "44.0.2",
    "lint-staged": "13.0.3",
    "micromatch": "4.0.5",
    "prettier": "2.7.1",
    "rollup": "2.79.1",
    "rollup-plugin-analyzer": "4.0.0",
    "rollup-plugin-node-externals": "5.0.1",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-ts": "3.0.2",
    "sort-package-json": "2.0.0",
    "typescript": "4.8.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
