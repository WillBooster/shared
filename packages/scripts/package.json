{
  "name": "@willbooster/shared-scripts",
  "version": "0.0.0-semantically-released",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "type": "module",
  "bin": {
    "wb": "bin/index.js"
  },
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "build-ts app",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "lint": "eslint --color \"./{scripts,src,tests}/**/*.{cjs,cts,js,jsx,mjs,mts,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prettify": "prettier --cache --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,js,json,json5,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "start": "build-ts run src/index.ts --",
    "start-prod": "yarn build && yarn wb",
    "test": "WB_ENV=test vitest run tests",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "@yarnpkg/shell": "4.0.0-rc.39",
    "chalk": "5.2.0",
    "minimal-promise-pool": "2.1.5"
  },
  "devDependencies": {
    "@types/eslint": "8.21.1",
    "@types/micromatch": "4.0.2",
    "@types/node": "18.14.0",
    "@types/yargs": "17.0.22",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "@willbooster/eslint-config-ts": "10.0.8",
    "@willbooster/prettier-config": "9.1.1",
    "blitz": "2.0.0-beta.23",
    "build-ts": "5.7.3",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-sort-class-members": "1.16.0",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-unicorn": "45.0.2",
    "lint-staged": "13.1.2",
    "micromatch": "4.0.5",
    "prettier": "2.8.4",
    "sort-package-json": "2.4.1",
    "type-fest": "3.6.0",
    "typescript": "4.9.5",
    "vitest": "0.28.5",
    "yargs": "17.7.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
