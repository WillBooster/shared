{
  "name": "@willbooster/wb",
  "version": "0.0.0-semantically-released",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "type": "module",
  "bin": "bin/index.js",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "yarn start buildIfNeeded --command 'yarn build-ts app'",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "lint": "eslint --color \"./{scripts,src,tests}/**/*.{cjs,cts,js,jsx,mjs,mts,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prettify": "prettier --cache --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,js,json,json5,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "start": "build-ts run src/index.ts --",
    "start-prod": "yarn build && yarn wb",
    "test": "WB_ENV=test vitest run tests",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "chalk": "5.3.0",
    "dotenv": "16.3.1",
    "kill-port": "2.0.1",
    "minimal-promise-pool": "4.0.0",
    "tree-kill": "1.2.2"
  },
  "devDependencies": {
    "@types/eslint": "8.44.2",
    "@types/kill-port": "2.0.1",
    "@types/micromatch": "4.0.2",
    "@types/node": "18.17.18",
    "@types/yargs": "17.0.24",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "@willbooster/eslint-config-ts": "10.5.0",
    "@willbooster/prettier-config": "9.1.1",
    "build-ts": "9.1.5",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-sort-class-members": "1.18.0",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-unicorn": "48.0.1",
    "lint-staged": "14.0.1",
    "micromatch": "4.0.5",
    "prettier": "3.0.3",
    "sort-package-json": "2.5.1",
    "type-fest": "4.3.1",
    "typescript": "5.2.2",
    "vitest": "0.34.5",
    "yargs": "17.7.2"
  },
  "engines": {
    "node": ">= 18"
  },
  "publishConfig": {
    "access": "public"
  }
}
