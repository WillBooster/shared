{
  "name": "@willbooster/shared-lib-react",
  "version": "0.0.0-semantically-released",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "build-ts lib",
    "build-storybook": "build-storybook",
    "check-all": "yarn cleanup && yarn typecheck && yarn test",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "lint": "eslint --color",
    "lint-fix": "yarn lint --fix --rule \"{ react-hooks/exhaustive-deps: 0 }\"",
    "prettify": "prettier --cache --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,js,json,json5,jsonc,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "storybook": "start-storybook -p 6006",
    "test/ci": "yarn build-storybook",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "devDependencies": {
    "@babel/core": "7.26.10",
    "@mdx-js/react": "3.1.0",
    "@storybook/addon-actions": "8.6.12",
    "@storybook/addon-docs": "8.6.12",
    "@storybook/addon-essentials": "8.6.12",
    "@storybook/addon-interactions": "8.6.12",
    "@storybook/addon-links": "8.6.12",
    "@storybook/builder-webpack4": "6.5.16",
    "@storybook/manager-webpack4": "6.5.16",
    "@storybook/react": "8.6.12",
    "@storybook/testing-library": "0.2.2",
    "@types/eslint": "8.56.10",
    "@types/micromatch": "4.0.9",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "@willbooster/eslint-config-ts-react": "11.2.0",
    "@willbooster/prettier-config": "10.0.0",
    "babel-loader": "10.0.0",
    "build-ts": "13.1.40",
    "eslint": "9.25.1",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-import-resolver-typescript": "4.3.4",
    "eslint-plugin-import-x": "4.11.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-sort-class-members": "1.21.0",
    "eslint-plugin-sort-destructure-keys": "2.0.0",
    "eslint-plugin-storybook": "0.12.0",
    "eslint-plugin-unicorn": "58.0.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "globals": "16.0.0",
    "lint-staged": "15.5.1",
    "micromatch": "4.0.8",
    "prettier": "3.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sort-package-json": "3.0.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.0",
    "vitest": "3.1.2"
  },
  "peerDependencies": {
    "react": "~18.2.0 || ~18.3.0 || ~19.0.0 || ~19.1.0",
    "react-dom": "~18.2.0 || ~18.3.0 || ~19.0.0 || ~19.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
