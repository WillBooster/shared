{
  "name": "@willbooster/shared-lib-react",
  "version": "0.0.0-semantically-released",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "build-ts lib",
    "build-storybook": "build-storybook",
    "check-all-for-ai": "yarn check-for-ai && yarn test",
    "check-for-ai": "yarn install > /dev/null && yarn format > /dev/null 2> /dev/null || true && yarn lint-fix --quiet && yarn typecheck",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "lint": "eslint --color",
    "lint-fix": "yarn lint --fix --rule \"{ react-hooks/exhaustive-deps: 0 }\"",
    "prettify": "prettier --cache --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,java,js,json,json5,jsonc,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/test{-,/}fixtures/**\" || true",
    "storybook": "start-storybook -p 6006",
    "test/ci": "yarn build-storybook",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "devDependencies": {
    "@babel/core": "7.28.4",
    "@mdx-js/react": "3.1.1",
    "@storybook/addon-actions": "9.0.8",
    "@storybook/addon-docs": "9.1.10",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-interactions": "8.6.14",
    "@storybook/addon-links": "9.1.10",
    "@storybook/builder-webpack4": "6.5.16",
    "@storybook/manager-webpack4": "6.5.16",
    "@storybook/react": "9.1.10",
    "@storybook/testing-library": "0.2.2",
    "@types/eslint": "9.6.1",
    "@types/micromatch": "4.0.9",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "@willbooster/eslint-config-ts-react": "11.5.6",
    "@willbooster/prettier-config": "10.2.0",
    "babel-loader": "10.0.0",
    "build-ts": "16.0.8",
    "eslint": "9.33.0",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-sort-class-members": "1.21.0",
    "eslint-plugin-sort-destructure-keys": "2.0.0",
    "eslint-plugin-storybook": "9.1.10",
    "eslint-plugin-unicorn": "60.0.0",
    "eslint-plugin-unused-imports": "4.2.0",
    "globals": "16.4.0",
    "lint-staged": "16.2.3",
    "micromatch": "4.0.8",
    "prettier": "3.6.2",
    "prettier-plugin-java": "2.7.5",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "sort-package-json": "3.4.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.45.0",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "react": "~18.2.0 || ~18.3.0 || ~19.0.0 || ~19.1.0 || ~19.2.0",
    "react-dom": "~18.2.0 || ~18.3.0 || ~19.0.0 || ~19.1.0 || ~19.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
