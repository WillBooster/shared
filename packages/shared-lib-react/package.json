{
  "name": "@willbooster/shared-lib-react",
  "version": "0.0.0-semantically-released",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "build-ts lib",
    "build-storybook": "build-storybook",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "lint": "eslint --color \"./{scripts,src,tests}/**/*.{cjs,cts,js,jsx,mjs,mts,ts,tsx}\"",
    "lint-fix": "yarn lint --fix --rule \"{ react-hooks/exhaustive-deps: 0 }\"",
    "prettify": "prettier --cache --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,js,json,json5,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "storybook": "start-storybook -p 6006",
    "test/ci": "yarn build-storybook",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "devDependencies": {
    "@babel/core": "7.22.5",
    "@mdx-js/react": "2.3.0",
    "@storybook/addon-actions": "7.0.24",
    "@storybook/addon-docs": "7.0.24",
    "@storybook/addon-essentials": "7.0.24",
    "@storybook/addon-interactions": "7.0.24",
    "@storybook/addon-links": "7.0.24",
    "@storybook/builder-webpack4": "6.5.16",
    "@storybook/manager-webpack4": "6.5.16",
    "@storybook/react": "7.0.24",
    "@storybook/testing-library": "0.2.0",
    "@types/eslint": "8.40.2",
    "@types/micromatch": "4.0.2",
    "@types/prettier": "2.7.3",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "@willbooster/eslint-config-ts-react": "10.1.3",
    "@willbooster/prettier-config": "9.1.1",
    "babel-loader": "9.1.2",
    "build-ts": "5.10.4",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-sort-class-members": "1.18.0",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-storybook": "0.6.12",
    "eslint-plugin-unicorn": "47.0.0",
    "lint-staged": "13.2.3",
    "micromatch": "4.0.5",
    "prettier": "3.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sort-package-json": "2.5.1",
    "typescript": "5.1.6",
    "vitest": "0.32.2"
  },
  "peerDependencies": {
    "react": "~18.2.0",
    "react-dom": "~18.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
